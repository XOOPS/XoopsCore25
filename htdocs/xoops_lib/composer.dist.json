{
  "name": "xoopscore25/libraries",
  "description": "Libraries for XOOPS 2.5.12 (meta-package, no code)",
  "type": "metapackage",
  "license": "GPL-2.0-or-later",
  "homepage": "https://xoops.org",
  "authors": [
    {
      "name": "XOOPS Project",
      "email": "contact@xoops.org",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-iconv": "*",
    "ext-mbstring": "*",

    "boenrobot/money_format_polyfill": "^1.0",
    "dflydev/apache-mime-types": "^1.0",
    "erusev/parsedown": "^1.7",
    "ezyang/htmlpurifier": "^4.18",
    "firebase/php-jwt": "^7.0",
    "php-debugbar/php-debugbar": "^v3.4.0",
    "monolog/monolog": "^3.0",
    "phpmailer/phpmailer": "^6.10",
    "psr/log": "^3.0",
    "punic/punic": "^3.8",
    "smarty/smarty": "^4.5",
    "smottt/wideimage": "^1.1",
    "tecnickcom/tcpdf": "^6.10",
    "xoops/regdom": "^2.1.0",
    "xoops/xmf": "^1.3.0"
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "classmap-authoritative": true
  },
  "minimum-stability": "beta",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/XOOPS/XoopsCore25/issues",
    "source": "https://github.com/XOOPS/XoopsCore25"
  }
}
