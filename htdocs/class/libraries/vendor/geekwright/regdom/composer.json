{
  "name": "geekwright/regdom",
  "description": "OO adaptation of Florian Sager's regdom library for querying Mozilla Public Suffix List",
  "keywords": [
    "public",
    "suffix",
    "tld",
    "regdom"
  ],
  "homepage": "https://github.com/geekwright/RegDom",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Richard Griffith",
      "email": "richard@geekwright.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "symfony/polyfill-mbstring": "^1.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.7"
  },
  "autoload": {
    "psr-4": {
      "Geekwright\\RegDom\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Geekwright\\Regdom\\": "tests/unit/"
    }
  },

  "bin": ["bin/reloadpsl"]
}
